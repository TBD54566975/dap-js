{
  "name": "@tbd54566975/dap",
  "version": "0.0.2",
  "description": "TypeScript SDK for DAPs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "bun run build.ts && bun run build:types",
    "build:types": "tsc --project tsconfig.types.json"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/TBD54566975/dap-js",
  "bugs": "https://github.com/TBD54566975/dap-js/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TBD54566975/dap-js.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "DAP",
    "decentralized-identity",
    "paytags"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@web5/common": "1.0.1",
    "@web5/crypto": "1.0.1",
    "@web5/dids": "1.1.1",
    "typeid-js": "0.7.0"
  },
  "devDependencies": {
    "@tsconfig/bun": "1.0.6",
    "@types/bun": "latest",
    "typescript": "5.5.2"
  }
}